{
    "name": "vue3-perfect-scrollbar",
    "version": "2.0.0",
    "license": "MIT",
    "description": "Vue.js wrapper for perfect scrollbar",
    "paths": {
        "source": "src",
        "destination": "dist",
        "example": "example"
    },
    "main": "dist/vue3-perfect-scrollbar.cjs.js",
    "module": "dist/vue3-perfect-scrollbar.esm.js",
    "style": "dist/vue3-perfect-scrollbar.css",
    "types": "./types/vue3-perfect-scrollbar.d.ts",
    "scripts": {
        "build": "rollup -c --environment production",
        "build:css": "postcss src/style.css -o dist/vue3-perfect-scrollbar.min.css --env production && postcss src/style.css -o dist/vue3-perfect-scrollbar.css",
        "dev": "webpack-dev-server --config webpack.config.js",
        "test": "yarn jest",
        "test:dev": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ideacatlab/vue3-perfect-scrollbar"
    },
    "author": "Razvan Gheorghe <razvan@ideacat.ro>",
    "keywords": [
        "frontend",
        "scroll",
        "scrollbar",
        "vue"
    ],
    "devDependencies": {
        "@babel/core": "^7.22.7",
        "@babel/preset-env": "^7.22.7",
        "@lopatnov/rollup-plugin-uglify": "^2.1.5",
        "@npmcli/fs": "^3.1.0",
        "@rollup/plugin-buble": "1.0.2",
        "@rollup/plugin-commonjs": "^25.0.2",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@types/jest": "^29.5.2",
        "@vue/compiler-sfc": "^3.3.4",
        "@vue/test-utils": "^2.4.0",
        "babel-jest": "^29.6.0",
        "babel-loader": "^9.1.2",
        "babel-preset-jest": "29.5.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.44.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.15.1",
        "eslint-webpack-plugin": "^4.0.1",
        "jest": "^29.6.0",
        "jest-environment-jsdom": "^29.6.0",
        "memfs": "^4.2.0",
        "postcss": "^8.4.25",
        "postcss-cli": "^10.1.0",
        "quill-delta": "^5.1.0",
        "rollup": "^3.26.2",
        "rxjs": "7.8.1",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "terser": "^5.18.2",
        "thingies": "1.12.0",
        "ts-loader": "^9.4.4",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6",
        "vue": "^3.3.4",
        "vue-loader": "^17.2.2",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "cssnano": "^6.0.1",
        "perfect-scrollbar": "^1.5.5",
        "postcss-import": "^15.1.0"
    },
    "volta": {
        "node": "18.16.0"
    }
}
